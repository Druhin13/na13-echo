  "devDependencies": {
    "git-remote-origin-url": "^4.0.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "parcel-bundler": "^1.12.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
